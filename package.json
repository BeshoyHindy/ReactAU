  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack -p --colors --display-error-details --config=webpack/webpack.config.prod.js",
    "postprod": "npm-run-all prod:css prod:start",
    "prod:css": "concat -o public/build/css/bundle.css public/build/css/font-awesome.min.css public/build/css/bootstrap.min.css public/build/css/bundle.css ",
    "prod:start": "node server.generated.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "dev:dll": "webpack  --verbose --colors --display-error-details --config=webpack/webpack.config.dll.js",
    "dev": "concurrently --kill-others -p \"[{name}]\" -n \"Client,Server\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev:client\" \"npm run dev:server\" ",
    "dev:client": "node ./src/server/server.dev.start.js",
    "dev:server": "cross-env NODE_ENV=development webpack --watch --colors --display-error-details --config=./webpack/webpack.config.dev.server.js",
    "dev:start": "cross-env NODE_ENV=development nodemon --watch server.generate.dev.js ./server.generate.dev.js ",
    "stat": "webpack --progress --profile --json --config=webpack/webpack.config.dev.js > stats.json"
  },
